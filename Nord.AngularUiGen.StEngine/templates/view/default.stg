delimiters "%","%"

t1(model) ::= <<
Hello,  %salutationMap.(model.sex)% %model.lastName%
	
>>

t2(model) ::= <<
	%bodyMap.(model.sex)%
>>

femaleTemplate() ::= <<
Throwing a tupperware party! Won't you please join us?
>>

maleTemplate() ::= <<
Group fishing trip next week - see you there!
>>

salutationMap ::= [
	"female":"Ms.",
	"male":"Mr."
]

bodyMap ::= [
	"female": {femaleTemplate.()},
	"male": {maleTemplate.()}
]